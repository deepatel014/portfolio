<%- include('../partials/header') %>
<%- include('../partials/login_nav') %>
<div class="container">
  <h1><%= title %></h1>
  <a href="/business/add" class="btn btn-dark" role=""button>
    <i class="fas fa-plus-circle">Add a Contact</i>
  </a>
  <br>
  <br>
  <table class="table table">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Phone Number</th>
        <th scope="col">Email Address</th>
        <th scope="col">Update Contact</th>
        <th scope="col">Delete Contact</th>
      </tr>
    </thead>
    <tbody>
      
        <% for(let count=0; count < ContactList.length; count++) { %>
      <tr>
        <td><%= ContactList[count].name %></td> 
        <td><%= ContactList[count].phone %></td>
        <td><%= ContactList[count].email %></td>
        <td>
          <a href="/business/edit/" class="btn btn-dark">
            <i class="fas fa-pencil"> Edit</i>
          </a>
        </td>
        <td>
          <a href="/business/delete/" class="btn btn-dark">
            <i class="fas fa-minus-circle"> Delete</i>
          </a>
        </td>
      </tr>
      
      <% } %>
    </tbody>
  </table>
</div>

<%- include('../partials/footer') %>

<script>
  console.log(ContactList);
</script>